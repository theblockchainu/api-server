{
  "name": "collection",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "AddPeerRelation": {
      "relationName": [
        "reviews",
        "comments",
        "upvotes",
        "downvotes"
      ]
    },
    "HasOne": {
      "relations": [
        {
          "model": "calendar",
          "hasManyRelation": "calendars",
          "autoCreate": true
        }
      ]
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "title": {
      "type": "string"
    },
    "stage": {
      "type": "string"
    },
    "language": {
      "type": [
        "string"
      ]
    },
    "headline": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "prerequisites": {
      "type": "string"
    },
    "maxSpots": {
      "type": "string"
    },
    "videoUrl": {
      "type": "string"
    },
    "imageUrls": {
      "type": [
        "string"
      ]
    },
    "repeatFrequency": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "currency": {
      "type": "string"
    },
    "CancellationPolicy": {
      "type": "string"
    },
    "aboutHost": {
      "type": "string"
    },
    "notes": {
      "type": "string"
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "peer",
      "foreignKey": "peerId"
    },
    "views": {
      "type": "hasMany",
      "model": "view",
      "foreignKey": "sourceId",
      "through": "collection_has_view",
      "keyThrough": "targetId"
    },
    "topics": {
      "type": "hasMany",
      "model": "topic",
      "foreignKey": "sourceId",
      "through": "collection_has_topic",
      "keyThrough": "targetId"
    },
    "contents": {
      "type": "hasMany",
      "model": "content",
      "foreignKey": "sourceId",
      "through": "collection_has_content",
      "keyThrough": "targetId"
    },
    "reviews": {
      "type": "hasMany",
      "model": "review",
      "foreignKey": "sourceId",
      "through": "collection_has_review",
      "keyThrough": "targetId"
    },
    "participants": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "sourceId",
      "through": "collection_has_participant",
      "keyThrough": "targetId"
    },
    "invites": {
      "type": "hasMany",
      "model": "peer_invite",
      "foreignKey": "sourceId",
      "through": "collection_has_invite",
      "keyThrough": "targetId"
    },
    "calendars": {
      "type": "hasMany",
      "model": "calendar",
      "foreignKey": "sourceId",
      "through": "collection_has_calendar",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}