{
  "name": "answer",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "AddPeerRelation": {
      "relationName": [
        "comments",
        "upvotes",
        "downvotes",
        "views",
        "flags"
      ]
    },
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "text": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "peer": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "targetId",
      "through": "peer_has_answer",
      "keyThrough": "sourceId"
    },
    "questions": {
      "type": "hasMany",
      "model": "question",
      "foreignKey": "targetId",
      "through": "question_has_answer",
      "keyThrough": "sourceId"
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "sourceId",
      "through": "answer_has_comment",
      "keyThrough": "targetId"
    },
    "upvotes": {
      "type": "hasMany",
      "model": "upvote",
      "foreignKey": "sourceId",
      "through": "answer_has_upvote",
      "keyThrough": "targetId"
    },
    "downvotes": {
      "type": "hasMany",
      "model": "downvote",
      "foreignKey": "sourceId",
      "through": "answer_has_downvote",
      "keyThrough": "targetId"
    },
    "views": {
      "type": "hasMany",
      "model": "view",
      "foreignKey": "sourceId",
      "through": "answer_has_view",
      "keyThrough": "targetId"
    },
    "flags": {
      "type": "hasMany",
      "model": "flag",
      "foreignKey": "sourceId",
      "through": "answer_has_flag",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}
