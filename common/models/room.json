{
  "name": "room",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "AddPeerRelation": {
      "relationName": [
        "messages"
      ]
    },
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    },
    "GlobalUtils": {}
  },
  "properties": {
    "name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "messages": {
      "type": "hasMany",
      "model": "message",
      "foreignKey": "sourceId",
      "through": "room_has_message",
      "keyThrough": "targetId"
    },
    "participants": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "targetId",
      "through": "peer_has_joinedroom",
      "keyThrough": "sourceId"
    },
    "collection": {
      "type": "hasMany",
      "model": "collection",
      "foreignKey": "targetId",
      "through": "collection_has_room",
      "keyThrough": "sourceId"
    },
    "communities": {
      "type": "hasMany",
      "model": "community",
      "foreignKey": "targetId",
      "through": "community_has_room",
      "keyThrough": "sourceId"
    }
  },
  "acls": [],
  "methods": {}
}
