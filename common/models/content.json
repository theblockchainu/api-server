{
  "name": "content",
  "plural": "contents",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "HasOne": {
      "relations": [
        {
          "model": "schedule",
          "hasManyRelation": "schedules",
          "autoCreate": true
        },
        {
          "model": "location",
          "hasManyRelation": "locations",
          "autoCreate": true
        }
      ]
    },
    "AddPeerRelation": {
      "relationName": [
        "comments",
        "submissions",
        "views",
        "rsvps"
      ]
    },
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    },
    "GlobalUtils": {},
    "PatchMany": {
      "relations": [
        {
          "model": "availability",
          "hasManyRelation": "availabilities"
        }
      ]
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "supplementUrls": {
      "type": [
        "string"
      ]
    },
    "requireRSVP": {
      "type": "boolean"
    },
    "itemsProvided": {
      "type": [
        "string"
      ]
    },
    "notes": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string"
    },
    "prerequisites": {
      "type": "string"
    },
    "sessionIsApproved": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "collections": {
      "type": "hasMany",
      "model": "collection",
      "foreignKey": "targetId",
      "through": "collection_has_content",
      "keyThrough": "sourceId"
    },
    "schedules": {
      "type": "hasMany",
      "model": "schedule",
      "foreignKey": "sourceId",
      "through": "content_has_schedule",
      "keyThrough": "targetId"
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "sourceId",
      "through": "content_has_comment",
      "keyThrough": "targetId"
    },
    "locations": {
      "type": "hasMany",
      "model": "location",
      "foreignKey": "sourceId",
      "through": "content_has_location",
      "keyThrough": "targetId"
    },
    "submissions": {
      "type": "hasMany",
      "model": "submission",
      "foreignKey": "sourceId",
      "through": "content_has_submission",
      "keyThrough": "targetId"
    },
    "views": {
      "type": "hasMany",
      "model": "view",
      "foreignKey": "sourceId",
      "through": "content_has_view",
      "keyThrough": "targetId"
    },
    "rsvps": {
      "type": "hasMany",
      "model": "rsvp",
      "foreignKey": "sourceId",
      "through": "content_has_rsvp",
      "keyThrough": "targetId"
    },
    "availabilities": {
      "type": "hasMany",
      "model": "availability",
      "foreignKey": "sourceId",
      "through": "content_has_availability",
      "keyThrough": "targetId"
    },
    "peers": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "targetId",
      "through": "peer_has_content",
      "keyThrough": "sourceId"
    }
  },
  "acls": [],
  "methods": {}
}