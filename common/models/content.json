{
  "name": "content",
  "plural": "contents",
  "base": "PersistedModel",
  "idInjection": true,
  "mixins": {
    "HasOne": {
      "relations": [
        {
          "model": "schedule",
          "hasManyRelation": "schedules",
          "autoCreate": true
        }
      ]
    }
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "collections": {
      "type": "hasMany",
      "model": "collection",
      "foreignKey": "targetId",
      "through": "collection_has_content",
      "keyThrough": "sourceId"
    },
    "schedules": {
      "type": "hasMany",
      "model": "schedule",
      "foreignKey": "sourceId",
      "through": "content_has_schedule",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}